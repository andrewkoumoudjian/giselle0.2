{
  "name": "hr-candidate-filtering-system",
  "version": "0.2.0",
  "description": "HR Candidate Filtering System with Supabase and Vercel",
  "main": "api/index.js",
  "scripts": {
    "frontend:dev": "cd frontend && npm run dev",
    "backend:dev": "cd backend && python3 -m src.main",
    "dev": "concurrently \"npm run frontend:dev\" \"npm run backend:dev\"",
    "install:all": "npm install && cd frontend && npm install",
    "setup": "cd frontend && bash env-setup.sh",
    "start": "node api/index.js",
    "vercel:dev": "nodemon api/index.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "multer": "^1.4.5-lts.2",
    "openai": "^4.93.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "",
  "license": "MIT"
}
