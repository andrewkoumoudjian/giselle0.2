# API Keys
OPENROUTER_API_KEY=your_openrouter_api_key
ELEVENLABS_API_KEY=your_elevenlabs_api_key
OPENROUTER_MODEL=deepseek/deepseek-chat-v3-0324:free

# OpenAI API Key - Required for LLM functionality
OPENAI_API_KEY=your_openai_api_key

# Google Cloud Storage (required for Cloud Run)
GCS_BUCKET_NAME=giselle-uploads

# Server Configuration
PORT=8080
HOST=0.0.0.0
ENV=production

# JWT Configuration 
JWT_SECRET=your-jwt-secret
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# LLM Configuration
DEFAULT_MODEL=deepseek/deepseek-chat-v3-0324:free
TECHNICAL_ANALYSIS_MODEL=mistralai/mistral-nemo:free
RESUME_ANALYSIS_MODEL=google/gemini-2.5-pro-exp-03-25:free

# Database Mode - Set to false to enable real AI capabilities
USE_MOCK_DATA=true
USE_SUPABASE=false

# API Configuration
API_HOST=0.0.0.0
API_PORT=8080

# Speech API Configuration
ELEVENLABS_VOICE_ID=your_elevenlabs_voice_id 